{% extends 'model/_search.html.twig' %}

{% block search_form %}

<div class="col-auto my-1 px-1">
    <div class="form-check">{{ form_widget(form.lastname) }}</div>
    <div class="form-check">{{ form_widget(form.firstname) }}</div>
    <div class="form-check w-min-200">{{ form_widget(form.birthdate) }}</div>
</div>
{% endblock %}