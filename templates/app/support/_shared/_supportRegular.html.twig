<section class="form-group row">
	<div class="col-md-12"><h2 class="h5 mb-3">Orientation / pr√©-admission</h2></div>
    <div class="col-md-6">{{ form_row(form.originRequest.infoToSiaoDate) }}</div>
    <div class="col-md-6">{{ form_row(form.originRequest.orientationDate) }}</div>
    <div class="col-md-6">{{ form_row(form.originRequest.organization) }}</div>
    <div class="col-md-6" data-parent-field="orientationDate">{{ form_row(form.originRequest.organizationComment) }}</div>
    <div class="col-md-6" data-parent-field="orientationDate">{{ form_row(form.originRequest.preAdmissionDate) }}</div>
    <div class="col-md-6" data-parent-field="orientationDate">{{ form_row(form.originRequest.resulPreAdmission) }}</div>
    <div class="col-md-6" data-parent-field="orientationDate">{{ form_row(form.originRequest.decisionDate) }}</div>
    <div class="col-md-12" data-parent-field="orientationDate">{{ form_label(form.originRequest.comment) }}{{ form_widget(form.originRequest.comment) }}</div>
</section>
<section class="{% if support.device and support.device.place == YES %}d-none{% endif %}">
<hr>
    <div class="form-group row" id="support_location">
        <div class="col-md-12"><h2 class="h5 mb-3">Adresse du suivi</h2></div>
        <div class="col-md-6">{{ form_row(form.location.search) }}</div>
        <div class="col-md-6">{{ form_row(form.location.address) }}</div>
        <div class="col-md-6">{{ form_row(form.location.zipcode) }}</div>
        <div class="col-md-6">{{ form_row(form.location.city) }}</div>
        <div class="col-md-6">{{ form_row(form.location.commentLocation) }}</div>
        {{ form_row(form.location.locationId) }}
        {{ form_row(form.location.lat) }}
        {{ form_row(form.location.lon) }}
    </div>
</section>
<hr>
<section class="form-group">
    <div class="row">
        <div class="col-md-12"><h2 class="h5 mb-3">Accompagnement</h2></div>
        <div class="col-md-6">{{ form_row(form.status) }}</div>
        <div class="col-md-6">{{ form_row(form.startDate) }}</div>
    </div>
    {# {% if form.vars.value.placeGroups.count > 0 and form.placeGroups is defined %}
        <div class="row {% if support.device and support.device.place != YES %}d-none{% endif %}">
            <div class="col-md-6">{{ form_row(form.placeGroups[0].place) }}</div>
            <div class="col-md-6">{{ form_row(form.placeGroups[0].startDate) }}</div>
        </div>
    {% endif %} #}
    <div class="row">
        <div class="col-md-6" data-parent-field="startDate">{{ form_row(form.theoreticalEndDate) }}</div>
        <div class="col-md-6" data-parent-field="startDate">{{ form_row(form.endDate) }}</div>
    </div>
    <div class="row">
        <div class="col-md-6" data-parent-field="startDate">{{ form_row(form.endStatus) }}</div>
        <div class="col-md-6" data-parent-field="endStatus">{{ form_row(form.endStatusComment) }}</div>
    </div>
    <div class="row" data-parent-field="endStatus" id="support_end_location">
        <div class="col-md-6">{{ form_row(form.endLocationSearch) }}</div>
        <div class="col-md-6">{{ form_row(form.endLocationAddress) }}</div>
        <div class="col-md-6">{{ form_row(form.endLocationCity) }}</div>
        <div class="col-md-6">{{ form_row(form.endLocationZipcode) }}</div>
    </div>
    <div class="row" data-parent-field="endStatus">
        {% if support.id and support.device and support.device.place == YES %}
            <div class="col-md-6">
                <div class="custom-control custom-checkbox custom-checkbox-{{ theme_color }} pl-2 py-2 text-dark">
                    {{ form_row(form.endPlace) }}
                </div>
            </div>
        {% endif %}
    </div>
</section>
<div class="form-group row">
    <div class="col-md-12">{{ form_widget(form.comment) }}</div>
</div>