{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "cache/apcu-adapter": "^1.1",
        "cache/simple-cache-bridge": "^1.1",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.5.11",
        "dompdf/dompdf": "^1.2.1",
        "gedmo/doctrine-extensions": "3.*",
        "google/apiclient": "^2.12",
        "knplabs/knp-paginator-bundle": "^5.1",
        "microsoft/microsoft-graph": "^1.47",
        "monolog/monolog": "^1.26.1",
        "paragonie/sodium_compat": "^1.12",
        "php-webdriver/webdriver": "^1.8",
        "phpdocumentor/reflection-docblock": "^5.2",
        "phpmailer/phpmailer": "^6.1",
        "phpoffice/phpspreadsheet": "^1.10",
        "phpoffice/phpword": "^0.17.0",
        "sensio/framework-extra-bundle": "^5.4",
        "spatie/image-optimizer": "^1.3",
        "stof/doctrine-extensions-bundle": "^1.6",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "5.4.*",
        "symfony/cache": "5.4.*",
        "symfony/console": "5.4.*",
        "symfony/dotenv": "5.4.*",
        "symfony/event-dispatcher": "5.4.*",
        "symfony/expression-language": "5.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.4.*",
        "symfony/framework-bundle": "5.4.*",
        "symfony/http-client": "5.4.*",
        "symfony/intl": "5.4.*",
        "symfony/mailer": "5.4.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/process": "5.4.*",
        "symfony/property-access": "5.4.*",
        "symfony/property-info": "5.4.*",
        "symfony/proxy-manager-bridge": "5.4.*",
        "symfony/requirements-checker": "^1.1",
        "symfony/security-bundle": "5.4.*",
        "symfony/serializer": "5.4.*",
        "symfony/string": "5.4.*",
        "symfony/translation": "5.4.*",
        "symfony/twig-bundle": "5.4.*",
        "symfony/validator": "5.4.*",
        "symfony/web-link": "5.4.*",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "5.4.*",
        "tinify/tinify": "^1.5",
        "twig/cache-extra": "^3.3",
        "twig/cssinliner-extra": "^3.3",
        "twig/extra-bundle": "^3.3",
        "twig/intl-extra": "^3.0",
        "twig/string-extra": "^3.3",
        "twig/twig": "^3.3"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^6.3",
        "dbrekelmans/bdi": "^1.0",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "friendsofphp/php-cs-fixer": "^3.5.0",
        "fzaninotto/faker": "^1.9",
        "hautelook/alice-bundle": "^2.9",
        "liip/test-fixtures-bundle": "^2.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-symfony": "^1.1",
        "symfony/browser-kit": "5.4.*",
        "symfony/css-selector": "5.4.*",
        "symfony/debug-bundle": "5.4.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/panther": "1.1.*",
        "symfony/phpunit-bridge": "5.4.*",
        "symfony/stopwatch": "5.4.*",
        "symfony/web-profiler-bundle": "5.4.*",
        "theofidry/alice-data-fixtures": "^1.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "cache:warmup": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "requirements-checker": "script"
        },
        "cs-fix": [
            "./tools/php-cs-fixer/vendor/bin/php-cs-fixer fix src --verbose"
        ],
        "check-quality": [
            "./vendor/bin/phpstan analyse -c phpstan.neon"
        ],
        "database-setup": [
            "php bin/console make:migration --env=dev",
            "php bin/console doctrine:migrations:migrate --env=dev --no-interaction"
        ],
        "phpunit-tests-dev": [
            "php bin/console cache:clear --env=test",
            "php bin/console doctrine:database:drop -e test --force",
            "php bin/console doctrine:database:create -e test",
            "php bin/console doctrine:schema:update -e test --force",
            "php bin/console hautelook:fixtures:load -e test -n",
            "php bin/phpunit tests/Entity",
            "php bin/phpunit tests/Repository",
            "php bin/phpunit tests/Controller",
            "php bin/phpunit tests/EndToEnd"
        ],
        "phpunit-tests-prod": [
            "php bin/console cache:clear --env=prod",
            "php bin/console cache:clear --env=test",
            "php bin/console doctrine:database:drop -e test --force",
            "php bin/console doctrine:database:create -e test",
            "php bin/console doctrine:schema:update -e test --force",
            "php bin/console hautelook:fixtures:load -e test -n",
            "php bin/phpunit tests/Entity",
            "php bin/phpunit tests/Repository",
            "php bin/phpunit tests/Controller"
        ],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "scripts-descriptions": {
        "cs-fix": "Execute PHP CS Fixer in the src directory and fix errors",
        "check-quality": "Execute PHP Stan to check code quality"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*"
        }
    }
}
